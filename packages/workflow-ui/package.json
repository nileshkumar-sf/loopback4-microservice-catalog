{
  "name": "@sourceloop/workflow-ui",
  "version": "0.0.0",
  "description": "Library for providing a simple workflow builder",
  "keywords": [
    "angular-library"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "docs": "rm -rf ./docs && typedoc --out docs",
    "test": "ng test",
    "workflowBuild": "ng build workflow-ui  && cp -r projects/workflow-ui/src/assets projects/workflow-ui/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git",
    "directory": "packages/workflow-ui"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "rxjs": "^6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.10",
    "@angular/cdk": "^13.3.9",
    "@angular/cli": "~13.3.10",
    "@angular/compiler-cli": "~13.3.11",
    "@types/jasmine": "^3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "^4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.3.1",
    "typedoc": "^0.23.24",
    "typedoc-umlclass": "^0.5.0",
    "typescript": "4.6.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "directory": "dist"
  }
